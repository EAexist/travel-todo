{
    "cli": {
        "version": ">= 3.15.1",
        "appVersionSource": "local"
    },
    "build": {
        "production": {
            "node": "24.11.0",
            "environment": "production",
            "env": {
                "NODE_ENV": "production"
            },
            "android": {}
        },
        "development": {
            "environment": "development",
            "env": {
                "NODE_ENV": "development"
            },
            "extends": "production",
            "distribution": "internal",
            "android": {
                "gradleCommand": ":app:assembleDebug --stacktrace"
            },
            "ios": {
                "buildConfiguration": "Debug",
                "simulator": true
            }
        },
        "preview": {
            "environment": "preview",
            "extends": "production",
            "distribution": "internal",
            "ios": {
                "simulator": true
            },
            "android": {
                "buildType": "apk"
            }
        },
        "preview:device": {
            "environment": "preview",
            "extends": "preview",
            "ios": {
                "simulator": false
            }
        }
    },
    "submit": {
        "production": {}
    }
}